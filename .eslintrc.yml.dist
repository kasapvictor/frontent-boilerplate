env:
  browser: true
  es2021: true
extends:
  - airbnb-base
  - plugin:@typescript-eslint/recommended
parser: '@typescript-eslint/parser'
parserOptions:
  ecmaVersion: latest
  sourceType: module
plugins:
  - '@typescript-eslint'
rules: {
  "no-console": 0,
  "@typescript-eslint/semi": [
    "warn"
  ],
  "@typescript-eslint/no-empty-interface": [
    "error",
    {
      "allowSingleExtends": true
    }
  ]
}
